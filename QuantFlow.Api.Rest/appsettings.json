{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=QuantFlow;Trusted_Connection=true;TrustServerCertificate=true;"
  },
  "InfluxDb": {
    "TimeoutSeconds": 300,
    "EnableDebugLogging": false,
    "BatchSize": 1000,
    "FlushIntervalMs": 1000,
    "MaxRetries": 3
  },
  "Jwt": {
    "Key": "your-super-secret-jwt-key-that-is-at-least-32-characters-long",
    "Issuer": "QuantFlow.Api",
    "Audience": "QuantFlow.Users",
    "ExpirationHours": 24
  },
  "ApiKeys": [
    "dev-api-key-12345",
    "test-api-key-67890"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "QuantFlow": "Debug"
    },
    "Console": {
      "FormatterName": "json"
    }
  },
  "AllowedHosts": "*"
}