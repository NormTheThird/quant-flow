{
  "InfluxDb": {
    "TimeoutSeconds": 300,
    "EnableDebugLogging": false,
    "BatchSize": 1000,
    "FlushIntervalMs": 1000,
    "MaxRetries": 3
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Loki": {
    "Url": "http://localhost:3100"
  },
  "RateLimit": {
    "MaxRetries": 5,
    "BaseDelayMs": 1000,
    "RateLimitDelayMs": 15000,
    "MaxDelayMs": 300000,
    "MinIntervalBetweenCallsMs": 1200
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "DSharpPlus": "Information"
      }
    }
  }
}